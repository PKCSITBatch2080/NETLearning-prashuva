@model List<CollegeProgram>
<h1>All Programs</h1>
<table class="table">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">Name</th>
      <th scope="col">Affiliation</th>
      <th scope="col">Started</th>
    </tr>
  </thead>
  <tbody>
    @foreach(var college in Model){
    <tr>
      <th scope="row">@college.Id</th>
      <td>@college.Name</td>
      <td>@college.Affiliation</td>
      <td>@college.Started</td>
    </tr>
    }
  </tbody>
</table>