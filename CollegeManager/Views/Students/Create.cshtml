@model Student

@{
    ViewData["Title"] = "Create";
}
<h2>New Student Entry</h2>

<form asp-controller="Students" asp-action="Create" method="POST">
    <div>
        <label class="form-label">Name</label>
        <input asp-for="Name" type="text" class="form-control">
    </div>
    <div>
        <label class="form-label">Address</label>
        <input asp-for="Address" type="text" class="form-control">
    </div>
    <div>
        <label class="form-label">Email</label>
        <input asp-for="Email" type="email" class="form-control">
    </div>
    <div>
        <label class="form-label">DOB</label>
        <input asp-for="Dob" type="date" class="form-control">
    </div>

   <div class="form-group">
                <label asp-for="CollegeProgramId" class="control-label"></label>
                <select asp-for="CollegeProgramId" class="form-control" asp-items="ViewBag.CollegeProgramList"></select>
            </div>
    <button type="submit" class="btn btn-primary mt-2">Create</button>
</form>
<div>
    <a asp-action="Index">Back to List</a>
</div>